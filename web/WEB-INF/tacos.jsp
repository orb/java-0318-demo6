
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>The Best Tacos</title>
    </head>
    <body>
        <h1>Hello, Taco!</h1>

        <ul>
        <c:forEach var="taqueria" items="${taquerias}">
            <li>[${taqueria.id}] ${taqueria.name} rating: 
                <b><c:forEach begin="1" end="${taqueria.rating}">
                        <img src="data:image/jpeg;base64," width="25"/>
                </c:forEach></b>
                </li>
        </c:forEach>
        </ul>
    </body>
</html>
